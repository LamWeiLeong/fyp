<?php

// Check if the form is submitted

if ( @ $_POST["button btn-login"] == "Login") 
{
    $x = $_POST["email"];
    $y = $_POST["password"];
    if($x!="admin")
	{
		$a=mysqli_query ( $connect,"SELECT * FROM user_information WHERE email  = '{$x}'AND password = '{$y}'");
    	$row =mysqli_fetch_array($a);
		if( empty($row))
	{
		echo '<script>alert("Invalid Password or Username");</script>';
	}
	
	else
	{
		echo'email and Password is correct';
		$_SESSION['email']=$x;
		echo "<script>window.location.href='main_page.php';</script>";
	}

	}
	
}
?>

